{
    "SQL Version Header": {
        "prefix": "sql-version",
        "body": [
            "--",
            "-- VERSION ${1:1.0.0}",
            "--",
            "--",
            "-- VERSION     REQUEST NRO    USER            DATE             CHANGES",
            "-- ----------- -------------- --------------- ---------------- ----------------",
            "-- ${1:1.0.0}                      ${2:JMVELEZ}         ${3:2025-01-15}       ${4:INITIAL CREATION}",
            "-- ----------- -------------- --------------- ---------------- ----------------",
            "---",
            "--",
            "-- OBJECT TYPE: ${5:TABLE}",
            "-- OBJECT NAME: ${6:GE_TEXAMPLE}",
            "-- DESCRIPTION: ${7:Brief description of the object}",
            "--",
            "",
            "$0"
        ],
        "description": "SQL version header template for database objects"
    },
    "SQL Table Template": {
        "prefix": "sql-table",
        "body": [
            "--",
            "-- VERSION ${1:1.0.0}",
            "--",
            "--",
            "-- VERSION     REQUEST NRO    USER            DATE             CHANGES",
            "-- ----------- -------------- --------------- ---------------- ----------------",
            "-- ${1:1.0.0}                      ${2:JMVELEZ}         ${3:2025-01-15}       ${4:INITIAL CREATION}",
            "-- ----------- -------------- --------------- ---------------- ----------------",
            "---",
            "--",
            "-- OBJECT TYPE: TABLE",
            "-- OBJECT NAME: ${5:GE_TEXAMPLE}",
            "-- DESCRIPTION: ${6:Stores example data}",
            "--",
            "",
            "CREATE TABLE ${5:GE_TEXAMPLE} (",
            "    ${7:EXAMPLE_EXAMPLE} UUID PRIMARY KEY DEFAULT (UUID()),",
            "    ${8:NAME} VARCHAR(100) NOT NULL,",
            "    ${9:DESCRIPTION} TEXT,",
            "    IS_ACTIVE BOOLEAN DEFAULT TRUE,",
            "    CREATED_AT TIMESTAMP DEFAULT CURRENT_TIMESTAMP,",
            "    UPDATED_AT TIMESTAMP DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,",
            "    VERSION INTEGER DEFAULT 1",
            ");",
            "",
            "-- Indexes",
            "CREATE INDEX ${10:GE_IEXAMPLE_NAME} ON ${5:GE_TEXAMPLE}(${8:NAME});",
            "CREATE INDEX ${11:GE_IEXAMPLE_ACTIVE} ON ${5:GE_TEXAMPLE}(IS_ACTIVE);"
        ],
        "description": "Complete SQL table template with version header"
    },
    "SQL Stored Procedure Template": {
        "prefix": "sql-procedure",
        "body": [
            "--",
            "-- VERSION ${1:1.0.0}",
            "--",
            "--",
            "-- VERSION     REQUEST NRO    USER            DATE             CHANGES",
            "-- ----------- -------------- --------------- ---------------- ----------------",
            "-- ${1:1.0.0}                      ${2:JMVELEZ}         ${3:2025-01-15}       ${4:INITIAL CREATION}",
            "-- ----------- -------------- --------------- ---------------- ----------------",
            "---",
            "--",
            "-- OBJECT TYPE: PROCEDURE",
            "-- OBJECT NAME: ${5:GE_PEXAMPLE}",
            "-- DESCRIPTION: ${6:Example stored procedure}",
            "--",
            "",
            "DELIMITER //",
            "CREATE PROCEDURE ${5:GE_PEXAMPLE}(",
            "    IN p_${7:parameter} VARCHAR(50)",
            ")",
            "BEGIN",
            "    SELECT * FROM ${8:GE_TEXAMPLE} WHERE ${9:column} = p_${7:parameter};",
            "END//",
            "DELIMITER ;"
        ],
        "description": "SQL stored procedure template with version header"
    },
    "SQL View Template": {
        "prefix": "sql-view",
        "body": [
            "--",
            "-- VERSION ${1:1.0.0}",
            "--",
            "--",
            "-- VERSION     REQUEST NRO    USER            DATE             CHANGES",
            "-- ----------- -------------- --------------- ---------------- ----------------",
            "-- ${1:1.0.0}                      ${2:JMVELEZ}         ${3:2025-01-15}       ${4:INITIAL CREATION}",
            "-- ----------- -------------- --------------- ---------------- ----------------",
            "---",
            "--",
            "-- OBJECT TYPE: VIEW",
            "-- OBJECT NAME: ${5:GE_VEXAMPLE}",
            "-- DESCRIPTION: ${6:Example view}",
            "--",
            "",
            "CREATE VIEW ${5:GE_VEXAMPLE} AS",
            "SELECT ",
            "    ${7:column1},",
            "    ${8:column2},",
            "    ${9:column3}",
            "FROM ${10:GE_TEXAMPLE}",
            "WHERE IS_ACTIVE = TRUE;"
        ],
        "description": "SQL view template with version header"
    },
    "SQL Function Template": {
        "prefix": "sql-function",
        "body": [
            "--",
            "-- VERSION ${1:1.0.0}",
            "--",
            "--",
            "-- VERSION     REQUEST NRO    USER            DATE             CHANGES",
            "-- ----------- -------------- --------------- ---------------- ----------------",
            "-- ${1:1.0.0}                      ${2:JMVELEZ}         ${3:2025-01-15}       ${4:INITIAL CREATION}",
            "-- ----------- -------------- --------------- ---------------- ----------------",
            "---",
            "--",
            "-- OBJECT TYPE: FUNCTION",
            "-- OBJECT NAME: ${5:GE_FEXAMPLE}",
            "-- DESCRIPTION: ${6:Example function}",
            "--",
            "",
            "DELIMITER //",
            "CREATE FUNCTION ${5:GE_FEXAMPLE}(",
            "    p_${7:parameter} VARCHAR(50)",
            ") RETURNS VARCHAR(100)",
            "READS SQL DATA",
            "DETERMINISTIC",
            "BEGIN",
            "    DECLARE result VARCHAR(100);",
            "    SELECT ${8:column} INTO result FROM ${9:GE_TEXAMPLE} WHERE ${10:condition} = p_${7:parameter};",
            "    RETURN result;",
            "END//",
            "DELIMITER ;"
        ],
        "description": "SQL function template with version header"
    },
    "SQL Trigger Template": {
        "prefix": "sql-trigger",
        "body": [
            "--",
            "-- VERSION ${1:1.0.0}",
            "--",
            "--",
            "-- VERSION     REQUEST NRO    USER            DATE             CHANGES",
            "-- ----------- -------------- --------------- ---------------- ----------------",
            "-- ${1:1.0.0}                      ${2:JMVELEZ}         ${3:2025-01-15}       ${4:INITIAL CREATION}",
            "-- ----------- -------------- --------------- ---------------- ----------------",
            "---",
            "--",
            "-- OBJECT TYPE: TRIGGER",
            "-- OBJECT NAME: ${5:GE_TEXAMPLE_AUDIT_INSERT}",
            "-- DESCRIPTION: ${6:Audit trigger for example table}",
            "--",
            "",
            "DELIMITER //",
            "CREATE TRIGGER ${5:GE_TEXAMPLE_AUDIT_INSERT}",
            "AFTER INSERT ON ${7:GE_TEXAMPLE}",
            "FOR EACH ROW",
            "BEGIN",
            "    INSERT INTO ${8:GE_TAUDIT_LOG} (",
            "        TABLE_NAME,",
            "        ACTION_TYPE,",
            "        RECORD_ID,",
            "        NEW_VALUES,",
            "        CREATED_AT",
            "        CREATED_BY",
            "    ) VALUES (",
            "        '${7:GE_TEXAMPLE}',",
            "        'INSERT',",
            "        NEW.${9:EXAMPLE_EXAMPLE},",
            "        JSON_OBJECT(",
            "            '${10:column1}', NEW.${10:column1},",
            "            '${11:column2}', NEW.${11:column2}",
            "        ),",
            "        NOW(),",
            "        USER()",
            "    );",
            "END//",
            "DELIMITER ;"
        ],
        "description": "SQL trigger template with version header"
    }
} 